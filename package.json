{
  "name": "azure-keyvault-encrypted-config",
  "version": "0.6.0",
  "description": "Keep a json config file in your project which can contain encrypted secrets which will be decrypted via Azure KeyVault Key.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "bin": {
    "akec": "lib/commandline.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "author": "Philip Claren <philip@claren.io>",
  "license": "ISC",
  "dependencies": {
    "@azure/identity": "^2.0.4",
    "@azure/keyvault-keys": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "backend-tslint-rules": "1.14.0",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">= 10.17.0"
  }
}
